        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NofCode_RandomWalker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="SketchCanvas" Width="auto" Height="auto" Background="AliceBlue">
            <TextBlock x:Name="FrameCounterValue" FontSize="36" FontFamily="Segoe UI Light"/>
        </Canvas>
    </Grid>
</Window>
